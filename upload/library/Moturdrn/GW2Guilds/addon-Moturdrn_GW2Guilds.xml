    <template title="moturdrn_gw2guilds_add" version_id="1016" version_string="1.1.6"><![CDATA[<xen:title>{xen:if '{$guild.guild_id}', 'Edit Guild: {$guild.guild_name}', 'Add New Guild'}</xen:title>

<xen:if is="{$guild.guild_id}">
	<xen:navigation>
		<xen:breadcrumb href="{xen:link canonical:guilds, $guild}">{$guild.guild_name} [{$guild.guild_tag}]</xen:breadcrumb>
		<xen:breadcrumb href="{xen:link canonical:guilds/edit, $guild}">{xen:phrase moturdrn_gw2guilds_edit_guild}</xen:breadcrumb>
	</xen:navigation>
<xen:else />
	<xen:navigation>
		<xen:breadcrumb href="{xen:link canonical:guilds/add}">{xen:phrase moturdrn_gw2guilds_add_guild}</xen:breadcrumb>
	</xen:navigation>
</xen:if>
<div style="background-color: {xen:property contentBackground};">
<form action="{xen:link canonical:guilds/save, $guild}" method="post" class="AutoValidator xenForm" data-redirect="on">
	<xen:if is="!{$guild.guild_id}">
	<fieldset>	
		<dl class="ctrlUnit titleUnit">
			<dt><label for="ctrl_title">Guild Name:</label></dt>
			<dd>
				<input type="text" name="guild_name" value="{$guild.guild_name}" class="textCtrl OptOut"
					data-liveTitleTemplate="{xen:if $guild.guild_id, 'Edit Guild', 'Add New Guild'}: <em>%s</em>"
					id="ctrl_title_team_add" maxlength="100"
				/>
				<p class="explain">{xen:phrase moturdrn_gw2guilds_the_name_of_your_guild}</p>
			</dd>
		</dl>
		
		<dl class="ctrlUnit">
			<dt><label for="ctrl_tag_line">Guild Tag:</label></dt>
			<dd>
				<input type="text" name="guild_tag" value="{$guild.guild_tag}" class="textCtrl OptOut" id="ctrl_tag_line" maxlength="100" />
				<p class="explain">{xen:phrase moturdrn_gw2guilds_your_guilds_tag}</p>
			</dd>
		</dl>
		
		<dl class="ctrlUnit">
			<dt><label for="ctrl_tag_line">Website:</label></dt>
			<dd>
				<input type="text" name="guild_website" value="{$guild.guild_website}" class="textCtrl OptOut" id="ctrl_tag_line" maxlength="100" />
				<p class="explain">{xen:phrase moturdrn_gw2guilds_your_guilds_website}</p>
			</dd>
		</dl>
	</fieldset>
	<xen:else />
		<input type="hidden" name="guild_id" value="{$guild.guild_id}" />
		<dl class="ctrlUnit">
			<dt><label for="ctrl_tag_line">Website:</label></dt>
			<dd>
				<input type="text" name="guild_website" value="{$guild.guild_website}" class="textCtrl OptOut" id="ctrl_tag_line" maxlength="100" />
				<p class="explain">{xen:phrase moturdrn_gw2guilds_your_guilds_website}</p>
			</dd>
		</dl>
	</xen:if>
	
	<fieldset>
		<dl class="ctrlUnit">
			<dt><label for="ctrl_message">Members:</label></dt>
			<dd>
				<select name="members">
					<option value="1-50" <xen:if is="{$guild.members} == '1-50'">selected</xen:if>>1-50</option>
					<option value="51-100" <xen:if is="{$guild.members} == '51-100'">selected</xen:if>>51-100</option>
					<option value="101-150" <xen:if is="{$guild.members} == '101-150'">selected</xen:if>>101-150</option>
					<option value="151-200" <xen:if is="{$guild.members} == '151-200'">selected</xen:if>>151-200</option>
					<option value="201-250" <xen:if is="{$guild.members} == '201-250'">selected</xen:if>>201-250</option>
					<option value="251-300" <xen:if is="{$guild.members} == '251-300'">selected</xen:if>>251-300</option>
					<option value="301-350" <xen:if is="{$guild.members} == '301-350'">selected</xen:if>>301-350</option>
					<option value="351-400" <xen:if is="{$guild.members} == '351-400'">selected</xen:if>>351-400</option>
					<option value="401-450" <xen:if is="{$guild.members} == '401-450'">selected</xen:if>>401-450</option>
					<option value="451-500" <xen:if is="{$guild.members} == '451-500'">selected</xen:if>>451-500</option>
					<option value="501+" <xen:if is="{$guild.members} == '500+'">selected</xen:if>>500+</option>
				</select>
				<p class="explain">{xen:phrase moturdrn_gw2guilds_current_number_of_members}</p>
			</dd>
		</dl>
	</fieldset>
	
	<fieldset>
		<dl class="ctrlUnit">
			<dt><label for="ctrl_message">PvE:</label></dt>
			<dd>
				No: <input type="radio" name="PvE" value="N" <xen:if is="{$guild.PvE} == 'N' || !{$guild.guild_id}">checked</xen:if> /> Yes: <input type="radio" name="PvE" value="Y" <xen:if is="{$guild.PvE} == 'Y'">checked</xen:if> />
			</dd>
		</dl>
		<dl class="ctrlUnit">
			<dt><label for="ctrl_message">PvP:</label></dt>
			<dd>
				No: <input type="radio" name="PvP" value="N" <xen:if is="{$guild.PvP} == 'N' || !{$guild.guild_id}">checked</xen:if> /> Yes: <input type="radio" name="PvP" value="Y" <xen:if is="{$guild.PvP} == 'Y'">checked</xen:if> />
			</dd>
		</dl>
		<dl class="ctrlUnit">
			<dt><label for="ctrl_message">WvW:</label></dt>
			<dd>
				No: <input type="radio" name="WvW" value="N" <xen:if is="{$guild.WvW} == 'N' || !{$guild.guild_id}">checked</xen:if> /> Yes: <input type="radio" name="WvW" value="Y" <xen:if is="{$guild.WvW} == 'Y'">checked</xen:if> />
			</dd>
		</dl>
		<dl class="ctrlUnit">
			<dt><label for="ctrl_message">Casual:</label></dt>
			<dd>
				No: <input type="radio" name="Casual" value="N" <xen:if is="{$guild.Casual} == 'N' || !{$guild.guild_id}">checked</xen:if> /> Yes: <input type="radio" name="Casual" value="Y" <xen:if is="{$guild.Casual} == 'Y'">checked</xen:if> />
			</dd>
		</dl>
	</fieldset>
	
	<fieldset>
		<dl class="ctrlUnit">
			<dt><label for="ctrl_message">Recruitment/About:</label></dt>
			<dd>
				{xen:raw $editorTemplate}
				<p class="explain">{xen:phrase moturdrn_gw2guilds_provide_an_in_depth_description}</p>
			</dd>
		</dl>
	</fieldset>
	
	<dl class="ctrlUnit submitUnit">
		<dt></dt>
		<dd>
			<input type="submit" value="{xen:phrase save}" class="button primary" accesskey="s" />
		</dd>
	</dl>
	<input type="hidden" name="_xfToken" value="{$visitor.csrf_token_page}" />
</form>
</div>]]></template>
    <template title="moturdrn_gw2guilds_delete" version_id="1016" version_string="1.1.6"><![CDATA[<xen:title>Delete Guild: {$guild.guild_name} [{$guild.guild_tag}]</xen:title>

<xen:navigation>
	<xen:breadcrumb href="{xen:link canonical:guilds, $guild}">{$guild.guild_name}</xen:breadcrumb>
	<xen:breadcrumb href="{xen:link canonical:guilds/delete, $guild}">{xen:phrase moturdrn_gw2guilds_delete_guild}</xen:breadcrumb>
</xen:navigation>

<form action="{xen:link canonical:guilds/delete, $guild}" method="post" class="xenForm formOverlay">
	<dl class="ctrlUnit">
		<dt>{xen:phrase moturdrn_gw2guilds_you_are_about_to_delete}</dt>
		<dd>
			{xen:helper guildemblem, $guild}
			<br />
			{$guild.guild_name} [{$guild.guild_tag}]
		</dd>
	</dl>

	<dl class="ctrlUnit submitUnit">
		<dt></dt>
		<dd><input type="submit" value="Delete Guild" class="button primary" /></dd>
	</dl>
	<input type="hidden" name="user_group_id" value="{$guild.user_group_id}" />
	<input type="hidden" name="_xfConfirm" value="1" />
	<input type="hidden" name="_xfToken" value="{$visitor.csrf_token_page}" />
</form>]]></template>
    <template title="moturdrn_gw2guilds_guild_list_item" version_id="1016" version_string="1.1.6"><![CDATA[<li class="GuildListItem {$guild.status}" id="guild-{$guild.guild_id}">
	<div class="listBlock GuildImage">
		<div class="listBlockInner">
			{xen:helper guildemblem, $guild}

		</div>
	</div>
	<div class="listBlock main">
		<div class="listBlockInner">
			<h3 class="title">
				<a name="guild-{$guild.guild_id}"></a><a href="{xen:link canonical:guilds, $guild}" class="TeamIcon">{$guild.guild_name}</a>
				<div class="guildTitle">
					[{$guild.guild_tag}]
				</div>
			</h3>
			<xen:if is="{$guild.guild_website}">
				<div class="tagLine">
					<a href="{$guild.guild_website}">{$guild.guild_website}</a>
				</div>
			</xen:if>
			<div class="tagLine">
				{$guild.recruitmentShort}
			</div>
			<div class="tagLine">
				<xen:if is="{$guild.canEdit}">
					<xen:if is="{$guild.status} == 'Active' || {$guild.status} == 'Pending (Change)'">
						<a href="{xen:link canonical:guilds/inactivate, $guild}" class="callToAction OverlayTrigger"><span>Set Inactive</span></a> 
					</xen:if>
					<xen:if is="{$guild.status} == 'Inactive'">
						<a href="{xen:link canonical:guilds/activate, $guild}" class="callToAction OverlayTrigger"><span>Set Active</span></a> 
					</xen:if>
				</xen:if>
				<xen:if is="{$guild.canJoin}">
					<a href="{xen:link canonical:guilds/join, $guild}" class="callToAction OverlayTrigger"><span>Join</span></a> 
				</xen:if>
				<xen:if is="{$guild.canLeave}">
					<a href="{xen:link canonical:guilds/leave, $guild}" class="callToAction OverlayTrigger"><span>Leave</span></a> 
				</xen:if>
				<xen:if is="{$guild.canEdit}">
					<a href="{xen:link canonical:guilds/edit, $guild}" class="callToAction OverlayTrigger"><span>Edit</span></a> 
				</xen:if>
				<a href="{xen:link canonical:guilds/roster, $guild}" class="callToAction OverlayTrigger"><span>Roster<xen:if is="{$guild.canEdit} && {$guild.pending_count} > 0"> ({$guild.pending_count})</xen:if></span></a> 
				<xen:if is="{$guild.canTransfer}">
					<a href="{xen:link canonical:guilds/transfer, $guild}" class="callToAction OverlayTrigger"><span>Transfer</span></a> 
				</xen:if>
				<xen:if is="{$guild.canDelete}">
					<a href="{xen:link canonical:guilds/delete, $guild}" class="callToAction OverlayTrigger"><span>Delete</span></a>
				</xen:if>
			</div>
		</div>
	</div>
	<div class="listBlock GuildStats">
		<div class="listBlockInner">
			<div class="pairsJustified">
				<dl><dt>Members:</dt> <dd>{$guild.members}</dd></dl>
				<dl><dt>GH.eu Members:</dt> <dd>{$guild.member_count}</dd></dl>
				<dl><dt>PvE:</dt> <dd>{$guild.PvE}</dd></dl>
				<dl><dt>PvP:</dt> <dd>{$guild.PvP}</dd></dl>
				<dl><dt>WvW:</dt> <dd>{$guild.WvW}</dd></dl>
				<dl><dt>Casual:</dt> <dd>{$guild.Casual}</dd></dl>
				<dl><dt>Status:</dt> <dd>{$guild.status}</dd></dl>
			</div>
		</div>
	</div>
</li>
<li></li>
<xen:require css="moturdrn_gw2guilds_list.css" />]]></template>
    <template title="moturdrn_gw2guilds_guild_list_pending_item" version_id="1016" version_string="1.1.6"><![CDATA[<xen:require css="moturdrn_gw2guilds_member.css" />
<li id="guild-{$guild.guild_id}-{$user.user_id}" class="primaryContent memberListItem guildMemberListItem">
	<div class="userAvatar">
		{xen:helper guildemblem, $guild}
	</div>
	<div class="memberInfo">
		<h3 class="title">{$guild.guild_name} [{$guild.guild_tag}]</h3>
		<div class="tagLine muted">{$guild.status}
		<xen:if is="{$controlAction}">
			<div class="controlAction">
				<ul class="controlLink">
					{xen:raw $controlLinks}
				</ul>
			</div>
		</xen:if>
		</div>
		<div class="details muted">
			<span class="faint"></span>
		</div>
	</div>
</li>]]></template>
    <template title="moturdrn_gw2guilds_index" version_id="1009" version_string="1.0.9"><![CDATA[<xen:title>Guilds</xen:title>
<xen:topctrl><xen:if is="{$myGuilds} > 0"><a href="{xen:link canonical:guilds/mine}" class="callToAction"><span>My Guilds</span></a></xen:if> <xen:if is="{$canCreate}"><a href="{xen:link canonical:guilds/add}" class="callToAction OverlayTrigger"><span>Add Guild</span></a></xen:if></xen:topctrl>

<xen:require css="moturdrn_gw2guilds_list.css" />
<xen:require css="moturdrn_gw2guilds_list_mini.css" />
<div class="GuildListBlock">
<div>
	<xen:if is="{$pendingGuilds} && {$isGW2GuildsAdmin}">
	<div class="memberListMain">
	<div class="section">
		<ol class="memberListItems">
			<li class="primaryContent">Pending Guild Requests</li>
			<xen:foreach loop="$pendingGuilds" value="$guild">
				<xen:include template="moturdrn_gw2guilds_guild_list_pending_item">
					<xen:set var="$controlAction">1</xen:set>
					<xen:set var="$controlLinks">
						<xen:if is="{$guild.status} == 'Pending (Change)'">
							<li><a href="{xen:link canonical:guilds/activate, $guild}" class="OverlayTrigger"><span class="control promote">Set Active</span></a></li>
							<li><a href="{xen:link canonical:guilds/inactivate, $guild, 't={$visitor.csrf_token_page}'}" class="OverlayTrigger removeLink"><span class="control remove">Set Inactive</span></a></li>
						<xen:else />
							<li><a href="{xen:link canonical:guilds/activate, $guild}" class="OverlayTrigger"><span class="control promote">Set Active</span></a></li>
							<li><a href="{xen:link canonical:guilds/delete, $guild, 'user_group_id={$guild.user_group_id}'}" class="OverlayTrigger removeLink"><span class="control remove">Delete</span></a></li>
						</xen:if>
					</xen:set>
				</xen:include>
			</xen:foreach>
		</ol>
	</div>
	</div>
	</xen:if>
	<ol class="guildList">
	<xen:if is="{$guilds}">
		<xen:foreach loop="$guilds" value="$guild">
