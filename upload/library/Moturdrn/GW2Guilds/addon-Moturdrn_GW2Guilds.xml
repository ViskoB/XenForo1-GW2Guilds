			<xen:foreach loop="$members" value="$user">
				<xen:include template="moturdrn_gw2guilds_member_list_item">
					<xen:set var="$controlAction"><xen:if is="({$guild.accessLevel} >= 30 && {$visitor.user_id} != {$user.user_id})">1</xen:if></xen:set>
					<xen:set var="$controlLinks">
						<xen:if is="{$guild.accessLevel} >= 40">
							<li><a href="{xen:link canonical:guilds/members/promote, $guild, 'user_id={$user.user_id}'}" class="OverlayTrigger"><span class="control promote">Promote to Officer</span></a></li>
						</xen:if>
						<xen:if is="{$user.user_id} != {$visitor.user_id}">
							<xen:if is="{$guild.accessLevel} >= 30">
								<li><a href="{xen:link canonical:guilds/members/remove, $guild, 'user_id={$user.user_id}', 't={$visitor.csrf_token_page}'}" class="OverlayTrigger removeLink"><span class="control remove">Remove</span></a></li>
							</xen:if>
						</xen:if>
					</xen:set>
				</xen:include>
			</xen:foreach>
		<xen:else />
			<li class="primaryContent">There are no ordinary members</li>
		</xen:if>
	</ol>
</div>
</div>
</div>]]></template>
    <template title="moturdrn_gw2guilds_transfer" version_id="1016" version_string="1.1.6"><![CDATA[<xen:title>Transfer Guild: {$guild.guild_name} [{$guild.guild_tag}]</xen:title>

<xen:navigation>
